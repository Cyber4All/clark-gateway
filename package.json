{
  "name": "clark-gateway",
  "version": "3.8.2",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "nodemon",
    "serve": "node -r ts-node/register ./src/app.ts",
    "serve:debug": "node --inspect=5858 -r ts-node/register ./src/app.ts",
    "serve:prod": "node dist/app.js",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "pack": "npm run build && cp ./package.json ./dist && cd ./dist && zip -r ../dist.zip  ./* & cd ..",
    "zip": "cp ./package.json ./dist && cd ./dist && zip -r ../dist.zip  ./* & cd ..",
    "deploy": "eb deploy CLARK-Gateway-Production --label CLARK-Gateway-v$(node -p \"require('./package.json').version\")",
    "prepare": "husky install"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.0.39",
    "@types/express-http-proxy": "^1.5.2",
    "@types/express-jwt": "6.0.1",
    "@types/jsonwebtoken": "^7.2.3",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.17.55",
    "@types/raven": "^2.1.3",
    "@types/swagger-jsdoc": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "eslint": "^8.16.0",
    "husky": "^8.0.1",
    "prettier": "^3.0.3",
    "ts-node": "^10.8.0",
    "tslint": "^5.20.1",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "@sentry/node": "^5.30.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.17.1",
    "express-http-proxy": "^1.1.0",
    "express-jwt": "^6.0.0",
    "jsonwebtoken": "^8.3.0",
    "morgan": "^1.9.0",
    "nodemon": "^2.0.7",
    "socket.io": "^4.8.0",
    "swagger-jsdoc": "^3.5.0"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  }
}
